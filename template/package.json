{
    "engines": {
      "node": ">=10"
    },
    "scripts": {
      "prepublish": "pkg-ok",
      "test": "ava --verbose",
      "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
      "coverage": "nyc ava",
      "report": "nyc report --reporter=html"
    },
    "husky": {
      "hooks": {
        "pre-push": "cross-env npm test && eslint src/**",
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
      }
    },
    "keywords": [
      "SlimIO",
      "monitoring"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "nyc": {
      "check-coverage": true,
      "per-file": false,
      "lines": 98,
      "statements": 98,
      "functions": 98,
      "branches": 90
    },
    "devDependencies": {
      "@commitlint/cli": "^7.2.1",
      "@commitlint/config-conventional": "^7.1.2",
      "@escommunity/minami": "^1.0.0",
      "@slimio/eslint-config": "^1.1.8",
      "@types/node": "^10.12.5",
      "ava": "^0.25.0",
      "cross-env": "^5.2.0",
      "eslint": "^5.9.0",
      "husky": "^1.1.3",
      "jsdoc": "^3.5.5",
      "nyc": "^13.1.0",
      "pkg-ok": "^2.3.1"
    }
  }
