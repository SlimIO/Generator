{
    "engines": {
      "node": ">=10"
    },
    "scripts": {
      "prepublishOnly": "pkg-ok",
      "test": "ava --verbose",
      "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
      "coverage": "nyc ava",
      "report": "nyc report --reporter=html"
    },
    "husky": {
      "hooks": {
        "pre-push": "cross-env npm test && eslint src/**",
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
      }
    },
    "keywords": [
      "SlimIO",
      "monitoring"
    ],
    "author": "SlimIO",
    "license": "MIT",
    "nyc": {
      "check-coverage": true,
      "per-file": false,
      "lines": 98,
      "statements": 98,
      "functions": 98,
      "branches": 90
    }
  }
