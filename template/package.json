{
    "engines": {
      "node": ">=12"
    },
    "scripts": {
      "start": "node index.js",
      "prepublishOnly": "pkg-ok",
      "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose"
    },
    "husky": {
      "hooks": {
        "pre-push": "cross-env eslint index.js && npm test",
        "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
      }
    },
    "keywords": [],
    "author": "SlimIO",
    "license": "MIT",
    "nyc": {
      "check-coverage": true,
      "per-file": false,
      "lines": 98,
      "statements": 98,
      "functions": 98,
      "branches": 90
    }
  }
